# item = {'a':1,'b':1,'c':2,'d':2,'e':3}
# output = {1:['a', 'b'], 2:['c', 'd'],3:['e'],}

# g = [“greenone”, “greentwo”, “greenthree”]
# output = "green"
 
# s = [[1,2],[1,2,3],[4,5],[5,[6,7]]]
# output = [1, 2, 1, 2, 3, 4, 5, 5, [6, 7]]



s = [[1,2],[1,2,3],[4,5],[5,[6,7]]]

output = []

for item in s:
    if type(item) == list:
        for val in item:
            output.append(val)
    else:
        output.append(item)

print(output)

# output = [1, 2, 1, 2, 3, 4, 5, 5, 6, 7]